{
	"name": "revival-archive",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "node pages/pages.js && vite build",
		"preview": "vite preview",
		"buildview": "node pages/pages.js && vite build && vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"md": "node pages/pages.js",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-netlify": "^2.0.7",
		"@sveltejs/kit": "^1.22.1",
		"@types/marked": "^5.0.0",
		"@unocss/extractor-svelte": "^0.53.5",
		"@unocss/transformer-directives": "^0.53.5",
		"marked": "^5.1.1",
		"mdsvex": "^0.11.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"sass": "^1.63.6",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.5",
		"sveltekit-autoimport": "^1.7.0",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"unocss": "^0.53.5",
		"vite": "^4.4.2"
	},
	"packageManager": "pnpm@8.6.6"
}
